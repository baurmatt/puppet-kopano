{
  "name": "jhooyberghs-kopano",
  "version": "0.1.0",
  "author": "Jeroen Hooyberghs <jeroen@hooyberghs.org>",
  "summary": "This module will install and configure one or more of the Kopano server components",
  "license": "Apache-2.0",
  "source": "https://github.com/jhooyberghs/puppet-kopano.git",
  "project_page": "",
